import java.util.Scanner;
public class Mod6_Oefening1
{
	public static void main(String[] args)
	{
		Scanner keyboard = new Scanner(System.in);
		
		System.out.println("Voer een heel getal in:");
		
		int imputNumber = keyboard.nextInt();
		
		System.out.println("Het ingevoerde getal is");
		System.out.print(imputNumber);
	}
}
